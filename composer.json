{
    "name": "yafou/container",
    "require": {
        "php": "^7.3",
        "psr/container": "^1.0.0",
        "opis/closure": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.64",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "YaFou\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YaFou\\Container\\Tests\\": "tests/"
        }
    },
    "provide": {
        "psr/container-implementation": "^1.0.0"
    }
}
